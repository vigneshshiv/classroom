{
  "name": "classroom",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "test": "jest --watch",
    "lint": "next lint",
    "lint:fix": "next lint --fix",
    "format:source": "prettier --config .prettierrc --write \"{src}/**/*.{js,ts,jsx,tsx,css}\"",
    "prettier": "yarn format:source",
    "check-all": "npm-run-all prettier lint:fix",
    "docker-compose": "docker-compose up",
    "postinstall": "npx prisma generate",
    "prisma-studio": "dotenv -e .env.local npx prisma studio",
    "prisma-sync": "dotenv -e .env.local npx prisma db push",
    "prisma-seed": "dotenv -e .env.local npx prisma db seed",
    "prisma-migrate": "dotenv -e .env.local npx prisma migrate dev"
  },
  "prisma": {
    "seed": "ts-node --compiler-options {\"module\":\"CommonJS\"} prisma/seed.ts"
  },
  "dependencies": {
    "@next-auth/prisma-adapter": "1.0.5",
    "@next/font": "13.0.2",
    "@prisma/client": "4.5.0",
    "@tanstack/react-query": "4.14.5",
    "aws-sdk": "2.1238.0",
    "classnames": "2.3.2",
    "dotenv-cli": "6.0.0",
    "framer-motion": "7.6.1",
    "locomotive-scroll": "4.1.4",
    "next": "13.0.2",
    "next-auth": "4.16.2",
    "react": "18.2.0",
    "react-device-detect": "2.2.2",
    "react-dom": "18.2.0",
    "react-hook-form": "7.37.0",
    "react-icons": "4.4.0",
    "react-locomotive-scroll": "0.2.2",
    "ts-node": "10.9.1",
    "typewriter-effect": "2.19.0",
    "zustand": "4.1.4"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "5.16.5",
    "@testing-library/react": "13.4.0",
    "@types/node": "18.8.3",
    "@types/react": "18.0.21",
    "@types/react-dom": "18.0.6",
    "autoprefixer": "10.4.12",
    "eslint": "8.25.0",
    "eslint-config-next": "13.0.2",
    "eslint-config-prettier": "8.5.0",
    "jest": "29.1.2",
    "jest-environment-jsdom": "29.1.2",
    "postcss": "8.4.17",
    "prettier": "2.7.1",
    "tailwindcss": "3.1.8",
    "typescript": "4.8.4"
  },
  "lint-staged": {
    "*.{js,ts,jsx,tsx,css}": "yarn prettier"
  }
}
